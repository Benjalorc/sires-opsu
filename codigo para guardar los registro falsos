var i = 0;

var myTimerVar;

function myTimerFunction(){
	myTimerVar = setInterval(timerFunction, 5000);
}
function timerFunction(){


	if(i<100){

	var obj = snisArr[i];
	var data = "text/json;charset=utf-8," + encodeURIComponent(JSON.stringify(obj));

